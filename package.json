{
  "name": "@sinbad/mf-test",
  "scripts": {
    "start": "webpack serve --port 9999",
    "start:standalone": "webpack serve --env standalone",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "prepare": "husky install",
    "coverage": "cross-env BABEL_ENV=test jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently npm:lint"
    }
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/eslint-parser": "7.15.0",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@babel/runtime": "7.14.8",
    "@sinbad/mf-types": "0.0.11",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/react-router-dom": "5.1.8",
    "@types/systemjs": "6.1.0",
    "@types/testing-library__jest-dom": "5.14.1",
    "@types/webpack-env": "1.16.0",
    "babel-jest": "27.0.6",
    "concurrently": "6.2.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-ts-react-important-stuff": "3.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "jest-cli": "27.0.6",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "single-spa-react": "4.3.1",
    "ts-config-single-spa": "2.0.1",
    "typescript": "4.3.4",
    "webpack": "5.40.0",
    "webpack-cli": "4.7.2",
    "webpack-config-single-spa-react": "3.0.0",
    "webpack-config-single-spa-react-ts": "3.0.0",
    "webpack-config-single-spa-ts": "3.0.0",
    "webpack-dev-server": "4.0.0-rc.0",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0"
  }
}
